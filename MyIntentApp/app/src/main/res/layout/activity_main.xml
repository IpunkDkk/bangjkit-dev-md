<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:padding="16dp">

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/btn_move_activity"
        android:layout_marginBottom="16dp"
        android:text="@string/move_activity"
        />
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/btn_move_with_data"
        android:layout_marginBottom="16dp"
        android:text="@string/move_with_data"
        />
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/btn_move_with_object"
        android:layout_marginBottom="16dp"
        android:text="@string/move_with_object"
        />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/btn_dial_number"
        android:layout_marginBottom="16dp"
        android:text="@string/dial_number"
        />
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/btn_move_with_result"
        android:layout_marginBottom="16dp"
        android:text="@string/move_with_result"
        />
    <TextView
        android:id="@+id/tv_result"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="@string/result_from_activity"
        android:textSize="24sp" />
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/btn_result_from_activity"
        android:layout_marginBottom="16dp"
        android:text="@string/result_from_activity"
        />
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/btn_chose_like_number"
        android:layout_marginBottom="16dp"
        android:text="@string/choose_number"
        />
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/btn_data_received_"
        android:layout_marginBottom="16dp"
        android:text="@string/data_received"
        />
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/btn_object_received"
        android:layout_marginBottom="16dp"
        android:text="@string/object_received"
        />
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/btn_this_is_moveactivity"
        android:layout_marginBottom="16dp"
        android:text="@string/this_is_moveactivity"
        />

</LinearLayout>